{
  "name": "home-depot-system",
  "version": "0.0.1",
  "description": "The home depot system.",
  "main": "server.js",
  "scripts": {
    "test": "cross-env NODE_ENV=production isTest=true mocha tests/*.spec.js --exit",
    "wrap": "nodemon server --ignore tests/ --delay 2.5",
    "email-service": "cross-env NODE_ENV=production node services/app-master",
    "dev": "run-p wrap email-service",
    "lint": "eslint \"**/*.js\"",
    "serve": "cross-env NODE_ENV=production node server",
    "prod": "run-p serve email-service",
    "cluster": "cross-env NODE_ENV=production pm2 start ./app.config.js",
    "start": "cross-env NODE_ENV=production pm2-runtime start ./app.config.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "amqplib": "^0.6.0",
    "argon2": "^0.27.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fs-extra": "^9.0.1",
    "helmet": "^4.3.1",
    "http-status-codes": "^2.1.4",
    "ioredis": "^4.19.4",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^5.11.12",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "node-fetch": "^2.6.1",
    "nodemailer": "^6.4.17",
    "npm-run-all": "^4.1.5",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pm2": "^4.5.1",
    "rate-limiter-flexible": "^2.2.1",
    "validator": "^13.5.2"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^7.17.0",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.7",
    "supertest": "^4.0.2"
  }
}
